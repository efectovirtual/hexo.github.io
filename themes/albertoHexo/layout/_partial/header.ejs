<header>

<nav class="navbar navbar-inverse"> 

  <div class="container">

    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#menuTop" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span> 
      </button>
       <a href="<%- theme.menu.Inicio %>" class="navbar-brand">Blog AlbertoRD</a>
    </div> 

    <div class="navbar-collapse collapse navbar-right" id="menuTop" aria-expanded="false" style="height: 1px;">
      <ul class="nav navbar-nav">
        <% for (var i in theme.menu){ %>
          <li><a href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
        <% } %>
      </ul>
    </div>
    
  </div> 

</nav>

<div id="carousel-example-generic" class="carousel slide" data-ride="carousel">

    <div id="toptitle" class="inner">
      <h1 id="logo-wrap">
        <a href="<%- url_for() %>" id="logo"><%= theme.title %></a>
      </h1>
      <% if (theme.subtitle){ %>
        <h2 id="subtitle-wrap">
          <a href="<%- url_for() %>" id="subtitle"><%= theme.subtitle %></a>
        </h2>
      <% } %>
    </div>
    
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
        <li data-target="#carousel-example-generic" data-slide-to="1"></li>
        <li data-target="#carousel-example-generic" data-slide-to="2"></li>
        <li data-target="#carousel-example-generic" data-slide-to="3"></li>
      </ol>

      <!-- Wrapper for slides img-->
      <div class="carousel-inner" role="listbox">
        
        <div class="item active">
          <img src="<%= theme.img %>b001.jpg" alt="Slider1">
          <div class="carousel-caption">
          </div>
        </div>

        <div class="item">
          <img src="<%= theme.img %>b002.jpg" alt="Slider2">
          <div class="carousel-caption">
          </div>
        </div>

        <div class="item">
          <img src="<%= theme.img %>b003.jpg" alt="Slider3">
          <div class="carousel-caption">
          </div>
        </div>

        <div class="item">
          <img src="<%= theme.img %>b004.jpg" alt="Slider4">
          <div class="carousel-caption">
          </div>
        </div>

      </div>

      
  </div>

</header>